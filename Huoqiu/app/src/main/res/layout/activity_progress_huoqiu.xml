<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
                android:background="@color/white"
              android:layout_height="match_parent">
<com.lsl.huoqiu.widget.PieProgress
    xmlns:pie="http://schemas.android.com/apk/res-auto"
    android:id="@+id/progress"
    android:layout_width="100dp"
    pie:pie_progressOne="@color/colorPrimary"
    pie:pie_progressTwo="@color/darkorange"
    pie:pie_progressThree="@color/aqua"
    pie:pie_progressFour="@color/blueviolet"
    pie:pie_backGround="@color/gainsboro"
    pie:pie_progressWidth="10dp"
    android:layout_centerInParent="true"
    android:layout_height="100dp"/>
</RelativeLayout>